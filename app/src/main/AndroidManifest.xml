<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cecs453_final_project.quizmasters">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/quizmasters_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/quizmasters_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".QuestionDetailsActivity"></activity>
        <activity android:name=".QuizSelectActivity" />
        <activity android:name=".QuizResultActivity" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".AdminMainActivity" />
        <activity android:name=".AddQuestionActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>